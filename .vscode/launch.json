{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome against localhost, with sourcemaps",
        "type": "chrome",
        "request": "launch",
        "sourceMaps": true,
        "url": "http://localhost:8080/",
        "webRoot": "${workspaceRoot}",
        "sourceMapPathOverrides": {
          "webpack:///./*": "${webRoot}/*"
        }
      }
    ]
  }